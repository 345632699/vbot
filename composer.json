{
    "name": "hanson/vbot",
    "type": "library",
    "description": "高度自定义的微信机器人，能够实现群控制，自动回复，消息转发，防撤回，留言统计等功能",
    "keywords": [
        "微信",
        "weixin",
        "微信机器人",
        "机器人"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "HanSon",
            "email": "h@hanc.cc"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.2",
        "endroid/qrcode": "^1.7",
        "pimple/pimple": "^3.0",
        "illuminate/support": "^5.3",
        "nesbot/carbon": "^1.21",
        "aferrandini/phpqrcode": "^1.0",
        "symfony/console": "3.*"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Hanson\\Vbot\\": "src/"
        }
    }
}
